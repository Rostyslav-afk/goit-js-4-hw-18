{{#each students}}
  <tr>
    <th>{{id}}</th>
    <th>{{name}}</th>
    <th>{{age}}</th>
    <th>{{course}}</th>
    <th>{{#each skills}}
        {{this}}{{#unless @last}}, {{/unless}}
      {{/each}}
    </th>
    <th>{{email}}</th>
    <th>{{isEnrolled}}</th>
    <th><button class="editStudentButton" data-id="{{id}}">Редагувати</button></th>
    <th><button class="deleteStudentButton" data-id="{{id}}">Видалити</button></th>
  </tr>
{{/each}}